<!-- Ruby enabled HTML page for adding new record ~AMP -->
<h1 style="text-align:center" class="velotitle">Enter Your Bike Shop Details</h1>
<br class="clear"/>

<div class="booyah-box col-xs-10 col-xs-offset-1">

	<%= simple_form_for @place do |f| %>
		<%= f.input :name %>
		<%= f.input :address %>
		<%= f.input :description %>
		<br />
		<%= f.submit "Add", :class => "btn velobtn" %>
	<% end %>
</div>
